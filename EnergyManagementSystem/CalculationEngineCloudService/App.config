<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add type="System.Diagnostics.TextWriterTraceListener" name="TextWriter" initializeData="../CalculationEngine.log" />
      </listeners>
    </trace>
    <switches>
      <!-- 0 - Disabled
           1 = Error   - Gives error messages
           2 = Warning - Gives errors and warnings
           3 = Info    - Gives more detailed error information
           4 = Verbose - Gives verbose trace information.    -->
      <add name="TraceLevel" value="Info" />
      <add name="TraceError" value="Error" />
      <add name="TraceWarning" value="Warning" />
    </switches>

    <sources>
      <source name="System.ServiceModel"
              switchValue ="Information, ActivityTracing"
              propagateActivity="true">
        <listeners>
          <add name="sdt"
               type="System.Diagnostics.TextWriterTraceListener"
               initializeData ="CalculationEngineServiceTrace.e2e" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
  <connectionStrings>
    <add name="Azure_History_DB" connectionString="Server=tcp:ems-sql-service.database.windows.net,1433;Initial Catalog=CE_HISTORY;Persist Security Info=False;User ID=emsAdmin;Password=maliAdmin23;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>